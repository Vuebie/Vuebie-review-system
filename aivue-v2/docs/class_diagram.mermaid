        +String thumbnailPath
        +JSON metadata
        +analyze(AnalysisType)
        +generateThumbnail()
        +getTags()
    }
    
    class Analysis {
        +UUID id
        +UUID videoId
        +AnalysisType type
        +AnalysisStatus status
        +DateTime createdAt
        +DateTime completedAt
        +String resultsPath
        +JSON settings
        +String error
        +Integer processingTime
        +start()
        +cancel()
        +getResults()
    }
    
    class AnalysisResult {
